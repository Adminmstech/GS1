
@{
    ViewBag.Title = "StaffWorkingHours";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}
<main class="app-content">
    <div class="col-md-15">
        <div class="tile">

            <h2>Staff Working Hours</h2>
            <label>Month : </label>
           
            <select id="Month" name="Month" class="col-md-2">
                <option value="1">January</option>
                <option value="2">February</option>
                <option value="3">March</option>
                <option value="4">April</option>
                <option value="5">May</option>
                <option value="6">June</option>
                <option value="7">July</option>
                <option value="8">August</option>
                <option value="9">September</option>
                <option value="10">October</option>
                <option value="11">November</option>
                <option value="12">December</option>
            </select>
            <br />
            <label>Year : </label>
            <input type="number" id="Year" name="Year" class="col-md-2"/>
            <br />
            <button type="button" id="btnsub143">Submit</button>
            <br />
            <p></p>
            <div>
                <table class="table table-striped table-bordered table-active table-responsive-sm">
                    <thead>
                        <tr>

                            <th scope="col">Staff_Code</th>
                            <th scope="col">Staff_Name</th>
                            <th scope="col">Mobile</th>
                            <th scope="col">Designation</th>
                            <th scope="col">Hourly_Rate</th>
                            <th scope="col">ClockOut_Time</th>
                            <th scope="col">ClockIn_Time</th>
                            <th scope="col">Working_Hours</th>
                            <th scope="col">Earnings</th>

                        </tr>

                    <thead>
                    <tbody id="tbldata143">
                    </tbody>
                </table>
            </div>

        </div>
    </div>
</main>